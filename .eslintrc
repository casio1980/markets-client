{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "plugins": [
        "eslint-plugin-react"
    ],
    "rules": {
        "class-methods-use-this": 0,
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }],

        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 1
    },
    "env": {
        "browser": true
    }
}